

@model List<TblAbout>

<section class="section pt-0" id="about">
    <!-- container -->
    <div class="container text-center">
        <!-- about wrapper -->
        <div class="about">
            @foreach (var item in Model)
            {
                <div class="about-img-holder">
                    <img src="@item.ImageUrl" class="about-img">
                </div>
                <div class="about-caption">

                    <p class="section-subtitle">@item.Header</p>
                    <h2 class="section-title mb-3">@item.Title</h2>
                    <p>
                        @item.Description
                    </p>
                    <a href="/meyawo-1.0.0/assets/cv/Kadir GÜVEN - CV - Eng - FlowCv.pdf" download="cv_filename.pdf" class="btn-rounded btn btn-outline-primary mt-4">CV'mi indirebilirsiniz..</a>


                </div>
            }
        </div><!-- end of about wrapper -->
    </div><!-- end of container -->
</section>